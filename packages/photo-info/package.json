{
  "name": "photo-info",
  "version": "0.4.1",
  "type": "module",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "typecheck:test": "tsc --noEmit -p tsconfig.test.json"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "keywords": [
    "typescript",
    "photo",
    "exif",
    "field-of-view",
    "geo",
    "location"
  ],
  "homepage": "https://github.com/tyom/photo-info#readme",
  "repository": "https://github.com/tyom/photo-info",
  "bugs": {
    "url": "https://github.com/tyom/photo-info/issues"
  },
  "author": {
    "name": "Tyom Semonov",
    "email": "mail@tyom.net"
  },
  "license": "MIT",
  "dependencies": {
    "exifreader": "^4.31.1"
  },
  "devDependencies": {
    "tsup": "^8.3.0",
    "vitest": "^2.1.9"
  }
}
